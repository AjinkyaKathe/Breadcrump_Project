<div class="container">
<table class="table table-striped table-bordered">
<tr>
<th>Name</th>
<th>Contact</th>
<th>Email</th>
<th>Gender</th>
<th>Blood Group</th>
<th>Emergency No 1</th>
<th>Country</th>
<th>State</th>
<th>City</th>

</tr>

<%@user.each do |user| %>
<tr>
<td><%= user.name %></td>
<td><%= user.contact %></td>
<td><%= user.emial %></td>
<td><%= user.gender %></td>


<%@more_detail.each do |more|%>
<td><%= more.blood_group%></td>
<td><%= more.emergency_contact_1%></td>
<td><%= more.country%></td>
<td><%= more.state%></td>
<td><%= more.city%></td>
 <% break%>
<%end%> 
<%end%>
</tr>

</table>
</div>